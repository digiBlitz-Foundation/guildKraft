              </div><!--end controls-->
            </div><!--end control-group-->
			<div class="control-group">
              <label class="control-label" for="passwordinput"><strong>Phone</strong></label>
              <div class="controls">
			  
				<input type="text"  name="phone" class="form-control" onfocus="this.value = '';" class="input-medium"  placeholder="Phone Number" required/>
               
              </div><!--end controls-->
            </div>
           
		   <div class="control-group">
              <label class="control-label" for="passwordinput"><strong>Company</strong></label>
              <div class="controls">
			  
